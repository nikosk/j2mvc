<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <context-param>
        <param-name>DBCACHE</param-name>
        <param-value>TRUE</param-value>
    </context-param>
    <context-param>
        <param-name>PAGECACHE</param-name>
        <param-value>TRUE</param-value>
    </context-param>    
    <listener>
        <description>ee</description>
        <listener-class>gr.dsigned.jmvc.listeners.JmvcInitializationListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Adapter</servlet-name>
        <servlet-class>gr.dsigned.jmvc.JmvcApplicationController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Adapter</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>
